{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Orleans": "Error"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "HttpEndpoint": {
        "Url": "http://0.0.0.0:5000",
        "Protocols": "Http1AndHttp2"
      }
    }
  },
  "Orleans": {
    "AzureTableStorage": {
      "ConnectionString": "UseDevelopmentStorage=true"
    },
    "AzureQueue": {
      "ConnectionString": "UseDevelopmentStorage=true"
    }
  }
}